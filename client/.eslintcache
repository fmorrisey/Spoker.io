<table className=\"table\">\n          <thead className=\"thead-light\">\n            <tr>\n              <th>Name</th>\n              <th>Brand</th>\n              <th>Dept.</th>\n              <th>Category</th>\n              <th>MSRP / Retail</th>\n            </tr>\n          </thead>\n          <tbody>\n            { this.productList() }\n          </tbody>\n        </table>\n        </div>\n      </div>\n    );\n  }\n}","/Users/Morio-Kun/Code/dCC/spoker/project/mern-spoker/client/src/container/manager/sales.js",[],"/Users/Morio-Kun/Code/dCC/spoker/project/mern-spoker/client/src/container/layout/landing.js",[],"/Users/Morio-Kun/Code/dCC/spoker/project/mern-spoker/client/src/container/manager/orders.js",[],"/Users/Morio-Kun/Code/dCC/spoker/project/mern-spoker/client/src/container/inventory/product-edit.js",[],"/Users/Morio-Kun/Code/dCC/spoker/project/mern-spoker/client/src/container/inventory/product-add.js",[],{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","replacedBy":"58"},{"ruleId":"59","severity":1,"message":"60","line":53,"column":11,"nodeType":"61","messageId":"62","endLine":53,"endColumn":27},"no-native-reassign",["63"],"no-negated-in-lhs",["64"],"no-unused-vars","'isEmptyInventory' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]